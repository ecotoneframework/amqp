{
    "name": "ecotone/amqp",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "dgafka.mail@gmail.com"
        }
    ],
    "keywords": ["amqp","rabbitmq","rabbit","messaging", "asynchronous", "message", "enterprise integration patterns", "eip", "distributed architecture","domain driven design"],
    "description": "Extends Ecotone with AMQP integration",
    "autoload": {
        "psr-4": {
            "Ecotone\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ecotone\\": ["tests", "vendor/ecotone/ecotone/tests"]
        },
        "psr-0": {
            "FixtureAutoloadTest\\": "tests\\Messaging",
            "FixtureIncorrectNamespace\\": "tests\\Messaging"
        }
    },
    "require": {
        "php": ">=7.4",
        "ecotone/ecotone": "^0.12",
        "ecotone/enqueue": "^0.12",

        "enqueue/amqp-lib": "dev-master as 0.10.10",
        "enqueue/dsn": "^0.10.0",
        "enqueue/enqueue": "^0.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0.0",
        "behat/behat": "3.3.1",
        "phan/phan": "^1.2.0",
        "phpstan/phpstan": "^0.12"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../ecotone"
        },
        {
            "type": "path",
            "url": "../enqueue"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.11.x-dev"
        }
    }
}
